{
  "manifest_version": 2,

  "name": "Open All",
  "description": "This extension opens all of the posts from a subreddit page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "OpenAll"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}